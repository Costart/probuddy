{
  "name": "find-a-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "npx opennextjs-cloudflare build && wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply find-a-pro-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply find-a-pro-db --remote",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx tsx src/lib/db/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@google/generative-ai": "^0.24.1",
    "@microsoft/clarity": "^1.0.2",
    "@opennextjs/cloudflare": "^1.16.4",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "nanoid": "^5.1.6",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "openai": "^6.21.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-sqlite3": "^12.6.2",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.64.0"
  },
  "description": "Find a trusted pro near you"
}
